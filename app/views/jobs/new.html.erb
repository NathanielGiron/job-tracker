<a class="btn-floating btn-large waves-effect waves-light red"><i class="material-icons">view_list</i></a>
<h5 align="center"><i class="small material-icons">note_add</i> New Job Listing</h5>

<div class="row">
    <form class="col s12">
        <div class="row">
            <div class="input-field col s4">
                <i class="material-icons prefix">business</i>
                <input type="text" id="company_name" class="validate">
                <label for="company_name">Company Name</label>
            </div>
            <div class="input-field col s4">
                <i class="material-icons prefix">account_circle</i>
                <input type="text" id="job_title" class="validate">
                <label for="job_title">Job Title</label>
            </div>
            <div class="input-field col s4">
                <i class="material-icons prefix">info</i>
                <input type="text" id="job_number" class="validate">
                <label for="job_number">Job Number</label>
            </div>
        </div>
        
        <div class="row">
            <div class="input-field col s12">
                <input type="text" id="job_link" class="validate">
                <label for="job_link">Job Link</label>
            </div>
        </div>
        
        <div class="row">
            <div class="input-field col s6">
                <i class="material-icons prefix">today</i>
                <input type="text" id="interview_date" class="validate">
                <label for="date_applied">Date Applied</label>
            </div>
            <div class="input-field col s6">
                <i class="material-icons prefix">today</i>
                <input type="text" id="interview_date" class="validate">
                <label for="interview_date">Interview Date</label>
            </div>
        </div>
        
        <div class="row">
            <div class="input-field col s6">
                <i class="material-icons prefix">account_circle</i>
                <input type="text" id="contact_person" class="validate">
                <label for="contact_person">Contact Person</label>
            </div>
            <div class="input-field col s6">
                <i class="material-icons prefix">account_circle</i>
                <input type="text" id="contact_email" class="validate">
                <label for="contact_email">Contact Email</label>
            </div>
        </div>
        
        <div class="row">
            <input type="submit" class="waves-effect waves-light orange btn">
        </div>
    </form>
</div>